{
    "name": "pool-estimator",
    "version": "2.2.0",
    "description": "Professional Pool Cost Estimator",
    "author": "Sokratttttt",
    "repository": {
        "type": "git",
        "url": "https://github.com/sokratttttt/pool-estimator.git"
    },
    "private": true,
    "main": "electron/main.js",
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "lint:fix": "next lint --fix",
        "type-check": "tsc --noEmit",
        "type-check:strict": "tsc --noEmit --project tsconfig.strict.json",
        "type-check:watch": "tsc --noEmit --watch",
        "type-coverage": "node scripts/type-coverage.js",
        "find-any": "node scripts/find-any.js",
        "find-any:verbose": "node scripts/find-any.js --verbose",
        "type-report": "node scripts/generate-type-report.js",
        "validate": "npm run type-check && npm run lint",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "electron": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development electron .\"",
        "electron:build": "next build && electron-builder",
        "electron:build:win": "next build && electron-builder --win",
        "pack": "electron-builder --dir",
        "dist": "electron-builder",
        "prepare": "husky install"
    },
    "dependencies": {
        "@dnd-kit/core": "^6.3.1",
        "@dnd-kit/sortable": "^10.0.0",
        "@dnd-kit/utilities": "^3.2.2",
        "@google/generative-ai": "^0.24.1",
        "@react-pdf/renderer": "^4.3.1",
        "@react-three/drei": "^10.7.7",
        "@react-three/fiber": "^9.4.2",
        "@react-three/postprocessing": "^2.19.1",
        "@supabase/ssr": "^0.8.0",
        "@supabase/supabase-js": "^2.45.4",
        "@tanstack/react-query": "^5.90.12",
        "@tanstack/react-query-devtools": "^5.91.1",
        "@tanstack/react-virtual": "^3.13.12",
        "cmdk": "^1.1.1",
        "date-fns": "^4.1.0",
        "docx": "^9.5.1",
        "dotenv": "^17.2.3",
        "electron-log": "^5.4.3",
        "electron-updater": "^6.6.2",
        "exceljs": "^4.4.0",
        "file-saver": "^2.0.5",
        "framer-motion": "^11.0.0",
        "gantt-task-react": "^0.3.9",
        "html2canvas": "^1.4.1",
        "idb": "^8.0.3",
        "jspdf": "^3.0.4",
        "jspdf-autotable": "^3.8.2",
        "leaflet": "^1.9.4",
        "lucide-react": "^0.344.0",
        "next": "^14.2.33",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-leaflet": "^5.0.0",
        "react-window": "^2.2.3",
        "recharts": "^3.5.0",
        "sharp": "^0.34.5",
        "sonner": "^1.4.0",
        "three": "^0.160.1",
        "zustand": "^5.0.9"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/file-saver": "^2.0.7",
        "@types/node": "^24.10.1",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "@types/react-leaflet": "^2.8.3",
        "@types/react-window": "^1.8.8",
        "@types/three": "^0.181.0",
        "@typescript-eslint/eslint-plugin": "^8.48.0",
        "@typescript-eslint/parser": "^8.48.0",
        "autoprefixer": "^10.4.20",
        "concurrently": "^9.1.2",
        "cross-env": "^7.0.3",
        "electron": "^39.2.4",
        "electron-builder": "^25.1.8",
        "electron-is-dev": "^3.0.1",
        "eslint": "^8.57.1",
        "eslint-config-next": "^14.2.14",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "husky": "^8.0.0",
        "jest": "^30.2.0",
        "jest-environment-jsdom": "^30.2.0",
        "lint-staged": "^16.2.7",
        "postcss": "^8.4.47",
        "prettier": "^3.7.3",
        "tailwindcss": "^3.4.13",
        "typescript": "^5.9.3",
        "wait-on": "^8.0.1"
    },
    "build": {
        "appId": "com.mospool.calculator",
        "productName": "MOS-POOL Калькулятор",
        "directories": {
            "output": "dist"
        },
        "files": [
            "out/**/*",
            "electron/**/*",
            "public/**/*",
            "package.json"
        ],
        "win": {
            "target": [
                "nsis"
            ],
            "icon": "public/icon.ico"
        },
        "nsis": {
            "oneClick": false,
            "allowToChangeInstallationDirectory": true,
            "createDesktopShortcut": true,
            "createStartMenuShortcut": true,
            "shortcutName": "MOS-POOL Калькулятор"
        },
        "publish": {
            "provider": "github",
            "owner": "sokratttttt",
            "repo": "pool-estimator"
        }
    }
}
